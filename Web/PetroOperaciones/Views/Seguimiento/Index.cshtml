@model IEnumerable<PetroOperaciones.Models.Seguimiento>



<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            DocumentoOperaciones
        </th>
        <th>
            Usuario
        </th>
        <th>
            Observaciones
        </th>
        <th>
            FechaRegistro
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DocumentoOperaciones.NumeroDO)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Usuario.Cedula)
